<?xml version='1.0' encoding='utf-8'?>
<widget id="com.arkinnov.ektu" version="1.3.67" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>E KTU</name>
    <description>
        An educational app for KTU students
    </description>
    <author email="arkinnov@gmail.com" href="https://www.booksparrow.com/ektu/">
        BookSparrow Solutions
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="26" />
        <allow-intent href="market:*" />
        <icon src="www/images/ektuicon.png" />
    </platform>

    <engine name="android" spec="^6.2.3" />
    <!-- <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" /> -->
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <!-- <preference name="Orientation" value="portrait" /> -->


</widget>
